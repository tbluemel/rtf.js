{
    "compilerOptions": {
        "allowJs": false,
        "target": "es5",
        "lib": ["dom", "es5", "scripthost", "es2015.promise"],
        "moduleResolution": "node",
        "module": "es2015",
        "sourceMap": false,
        "alwaysStrict": true,
        "strictFunctionTypes": true,
        "strictPropertyInitialization": true,
        "noImplicitAny": true,
        "noImplicitThis": true
    }
}
